                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" bounces="NO" id="2yR-ps-hMF">
                                <rect key="frame" x="0.0" y="0.0" width="320" height="568"/>
